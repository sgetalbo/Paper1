keywords = {Classification,Clustering,Consumer classes,Data mining,Database systems,Decision trees,Electric loads,Electric power systems,Electric power utilization,Electricity consumer characterization,Learning systems,Load profiles,Mathematical models,Neural networks},
number = {2},
